{
	"$schema": "https://json.schemastore.org/tsconfig",
	"extends": "./base.json",
	"display": "Utils",

	"compilerOptions": {
		"isolatedModules": true,
		"esModuleInterop": true,
		"moduleResolution": "NodeNext",
		"module": "NodeNext",
		"resolveJsonModule": true,
		"strict": true,
		"allowJs": true,
		"skipLibCheck": true,
		"noEmit": false
	}
}
